
.weui-cells_after-title{
  padding: 10rpx 10rpx;
  background: #242424;
}

.weui-cell_input{
  display: flex;
  width: 100%;
  height: 100%;
  flex-direction: row;
  align-items: space-between;
  justify-content: center;
  position:relative;
}
.inim{
  width: 32rpx;
  height: 32rpx;
  position: absolute;
  top: 25rpx;
  left: 20rpx;
}
.weui-input{
  flex:1;
  background: #1a1a1a;
  height: 75rpx;
  font-size: 32rpx;
  color:#fff;
  padding-left:70rpx;
}
button{
  background: #242424;
  width: 75rpx;
  padding:0;
  height: 100%;
  font-size: 32rpx;
  color:#fff;
}